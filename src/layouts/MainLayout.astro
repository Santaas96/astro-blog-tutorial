---
import MainHead from "./MainHead.astro";

import "../styles/global.css";
import Footer from "../components/Footer.astro";
import Nav from "../components/Nav.astro";

const {
  title,
  description = "Default Description",
  image,
  frontmatter,
  robots,
} = Astro.props;
---

<html lang="en">
  <MainHead
    title={title}
    description={description}
    {image}
    {frontmatter}
    {robots}
  />
  <body>
    <Nav />
    <main>
      <slot />
    </main>
    <Footer />
  </body>
</html>
